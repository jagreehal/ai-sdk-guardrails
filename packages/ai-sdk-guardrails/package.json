{
  "name": "ai-sdk-guardrails",
  "version": "5.3.0",
  "description": "Input and output guardrails middleware for Vercel AI SDK.",
  "keywords": [
    "ai",
    "guardrails",
    "vercel-ai-sdk",
    "typescript",
    "validation",
    "safety",
    "streaming",
    "security",
    "content-filtering",
    "prompt-injection",
    "ai-safety"
  ],
  "main": "dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./guardrails/input": {
      "types": "./dist/guardrails/input.d.ts",
      "import": "./dist/guardrails/input.js",
      "require": "./dist/guardrails/input.cjs"
    },
    "./guardrails/output": {
      "types": "./dist/guardrails/output.d.ts",
      "import": "./dist/guardrails/output.js",
      "require": "./dist/guardrails/output.cjs"
    },
    "./guardrails/tools": {
      "types": "./dist/guardrails/tools.d.ts",
      "import": "./dist/guardrails/tools.js",
      "require": "./dist/guardrails/tools.cjs"
    }
  },
  "homepage": "https://github.com/jagreehal/ai-sdk-guardrails",
  "bugs": {
    "url": "https://github.com/jagreehal/ai-sdk-guardrails/issues"
  },
  "author": "Jag Reehal<jag@jagreehal.com> (https://jagreehal.com)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jagreehal/ai-sdk-guardrails.git"
  },
  "license": "MIT",
  "files": [
    "dist"
  ],
  "type": "module",
  "scripts": {
    "build": "tsup",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check src/**/*.ts",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "check-exports": "attw --pack . --ignore-rules false-esm no-resolution"
  },
  "dependencies": {
    "@ai-sdk/provider": "2.0.0",
    "ai": "5.0.109",
    "zod": "^4.1.13"
  },
  "peerDependencies": {
    "@opentelemetry/api": "^1.0.0"
  },
  "peerDependenciesMeta": {
    "@opentelemetry/api": {
      "optional": true
    }
  },
  "devDependencies": {
    "@ai-sdk/groq": "^2.0.32",
    "@ai-sdk/mistral": "2.0.25",
    "@ai-sdk/openai": "2.0.85",
    "@arethetypeswrong/cli": "^0.18.2",
    "@eslint/js": "^9.39.1",
    "@opentelemetry/api": "^1.9.0",
    "@total-typescript/ts-reset": "^0.6.1",
    "@total-typescript/tsconfig": "^1.0.4",
    "@types/eslint-config-prettier": "^6.11.3",
    "@types/node": "^25.0.0",
    "@typescript-eslint/eslint-plugin": "^8.49.0",
    "@typescript-eslint/parser": "^8.49.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-unicorn": "^62.0.0",
    "globals": "^16.5.0",
    "prettier": "^3.7.4",
    "tsup": "^8.5.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.49.0",
    "vitest": "^4.0.15"
  }
}
