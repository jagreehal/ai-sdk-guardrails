{
  "name": "ai-sdk-guardrails",
  "version": "4.0.0",
  "description": "Input and output guardrails middleware for Vercel AI SDK.",
  "keywords": [
    "ai",
    "guardrails",
    "vercel-ai-sdk",
    "typescript",
    "validation",
    "safety",
    "streaming",
    "security",
    "content-filtering",
    "prompt-injection",
    "ai-safety"
  ],
  "main": "dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./guardrails/input": {
      "types": "./dist/guardrails/input.d.ts",
      "import": "./dist/guardrails/input.js",
      "require": "./dist/guardrails/input.cjs"
    },
    "./guardrails/output": {
      "types": "./dist/guardrails/output.d.ts",
      "import": "./dist/guardrails/output.js",
      "require": "./dist/guardrails/output.cjs"
    }
  },
  "homepage": "https://github.com/jagreehal/ai-sdk-guardrails",
  "bugs": {
    "url": "https://github.com/jagreehal/ai-sdk-guardrails/issues"
  },
  "author": "Jag Reehal<jag@jagreehal.com> (https://jagreehal.com)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jagreehal/ai-sdk-guardrails.git"
  },
  "license": "MIT",
  "files": [
    "dist"
  ],
  "type": "module",
  "scripts": {
    "build": "tsup",
    "ci": "pnpm build && pnpm format:check && pnpm check-exports && pnpm lint && pnpm test",
    "lint": "eslint src/**/*.ts examples/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check src/**/*.ts examples/**/*.ts",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "check-format": "prettier --check .",
    "check-exports": "attw --pack . --ignore-rules false-esm no-resolution",
    "local-release": "pnpm build && pnpm format:check && pnpm check-exports && pnpm lint && pnpm test && changeset version && changeset publish",
    "release": "changeset publish",
    "quality": "pnpm lint && pnpm format && pnpm format:check && pnpm type-check"
  },
  "dependencies": {
    "@ai-sdk/provider": "2.0.0",
    "ai": "5.0.35",
    "zod": "^4.1.3"
  },
  "devDependencies": {
    "@ai-sdk/mistral": "2.0.12",
    "@ai-sdk/openai": "2.0.24",
    "@arethetypeswrong/cli": "^0.18.2",
    "@changesets/cli": "^2.29.6",
    "@total-typescript/ts-reset": "^0.6.1",
    "@total-typescript/tsconfig": "^1.0.4",
    "@types/eslint-config-prettier": "^6.11.3",
    "@types/inquirer": "^9.0.9",
    "@types/node": "^24.3.0",
    "@typescript-eslint/eslint-plugin": "^8.41.0",
    "@typescript-eslint/parser": "^8.41.0",
    "@eslint/js": "^9.34.0",
    "eslint": "^9.34.0",
    "ai-sdk-ollama": "^0.5.2",
    "autoevals": "^0.0.131",
    "dotenv": "^17.2.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-unicorn": "^60.0.0",
    "inquirer": "^12.9.4",
    "mathjs": "^14.6.0",
    "ollama": "^0.5.17",
    "ollama-ai-provider": "^1.2.0",
    "openai": "^5.15.0",
    "prettier": "^3.6.2",
    "tsup": "^8.5.0",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.41.0",
    "vitest": "^3.2.4"
  }
}
